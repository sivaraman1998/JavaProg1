		Vector <Object> vec=new Vector<Object>();
		
		vec.add("Siva");
		vec.add(54);
		vec.add(23.3);
		vec.add('S');
		
		System.out.println(vec);
		
		int size = vec.size();
		System.out.println(size);
		
		Object set1 = vec.set(3, "Set");
		System.out.println(vec);
		
		Object get1 = vec.get(3);
		System.out.println(get1);
		
