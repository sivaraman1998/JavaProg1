package com.employee;

public class Employee {
	public void empId() {
		System.out.println("Employee ID : "+3898);
	}
	public void empId(int id) {
		System.out.println("Employee ID : "+id);
	}
	public void empId(int id1, int id2) {
		System.out.println("Employee ID : "+id1+"	"+"Employee ID 2 :"+id2);
	}
	public static void main(String[] args) {
		Employee obj=new Employee();
		obj.empId();
		obj.empId(3899);
		obj.empId(3900, 3901);
	}
}
