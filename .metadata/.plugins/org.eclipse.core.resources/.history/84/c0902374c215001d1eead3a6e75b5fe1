package com.computer;

public class Desktop implements Hardware, Software{
	@Override
	public void hardwareResources() {
		System.out.println("Hardware resources typically includes I/O port addresses, interrupt vectors.");
		System.out.println();
	}
	@Override
	public void softwareResources() {
		System.out.println("It is a logical concept that is obtained by viewing the base hardware resources");
		System.out.println();
	}
	public void desktopModel(String s1) {
		System.out.println("Desktop model : "+s1);
	}
	public static void main(String[] args) {
		Desktop obj=new Desktop();
		obj.hardwareResources();
		obj.softwareResources();
		obj.desktopModel("DELL78253");
	}

}
